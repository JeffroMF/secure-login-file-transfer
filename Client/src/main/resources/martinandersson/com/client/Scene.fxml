<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="btnRegister" mnemonicParsing="false" text="Register user" />
      <Button fx:id="btnAuthenticate" mnemonicParsing="false" text="Authenticate user" />
      <Button fx:id="btnSendFile" mnemonicParsing="false" text="Send a file.." />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
